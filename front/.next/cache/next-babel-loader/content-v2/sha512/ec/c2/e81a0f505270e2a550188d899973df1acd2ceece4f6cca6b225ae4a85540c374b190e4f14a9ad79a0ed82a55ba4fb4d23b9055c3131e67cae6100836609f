  });\n    } else {\n      alert(\"입력 또는 버킷리스트를 확인하세요\");\n    }\n\n    setContent('');\n  }, [content]);\n  return /*#__PURE__*/_jsx(Form, {\n    children: /*#__PURE__*/_jsxs(CenterDiv, {\n      children: [/*#__PURE__*/_jsx(CustomBtn, {\n        onClick: onAddClick,\n        children: \"\\uCD94\\uAC00\"\n      }), /*#__PURE__*/_jsx(CustomBtn, {\n        onClick: onDeleteClick,\n        style: {\n          \"float\": 'right'\n        },\n        children: \"\\uC0AD\\uC81C\"\n      }), /*#__PURE__*/_jsx(CustomInput, {\n        onChange: onContentChange,\n        value: content,\n        placeholder: '추가 또는 삭제하고싶은 버킷리스트를 입력하세요'\n      }), /*#__PURE__*/_jsx(List, {\n        style: {\n          height: '350px',\n          overflow: 'scroll'\n        },\n        size: \"small\",\n        bordered: true,\n        dataSource: bucket,\n        renderItem: function renderItem(item) {\n          return /*#__PURE__*/_jsx(CustomItem, {\n            children: item.content\n          });\n        }\n      })]\n    })\n  });\n};\n\nexport default EditBucket;","map":null,"metadata":{},"sourceType":"module"}