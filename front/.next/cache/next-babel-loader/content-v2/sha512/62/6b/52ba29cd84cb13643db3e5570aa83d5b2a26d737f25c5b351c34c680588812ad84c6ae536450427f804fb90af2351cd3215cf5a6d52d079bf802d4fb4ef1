{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport { Form, Button, Input } from 'antd';\nimport styled from 'styled-components';\nimport { useCallback, useState, useEffect } from 'react';\nimport Router from 'next/router';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { SIGN_UP_REQUEST, SIGN_UP_RESET } from '../reducers/user';\nconst ButtonWrapper = styled.div.withConfig({\n  displayName: \"SignUp__ButtonWrapper\",\n  componentId: \"fugbur-0\"\n})([\"margin-top:10px;\"]);\nconst FormWrapper = styled(Form).withConfig({\n  displayName: \"SignUp__FormWrapper\",\n  componentId: \"fugbur-1\"\n})([\"position:absolute;top:50%;left:50%;width:300px;margin:-100px 0 0 -150px;\"]);\n\nconst LoginForm = () => {\n  const dispatch = useDispatch();\n  const {\n    isSignUpLoading,\n    isSignUpError,\n    isSignUpDone\n  } = useSelector(state => state.user);\n  const {\n    0: email,\n    1: setEmail\n  } = useState('');\n  const {\n    0: password,\n    1: setPassword\n  } = useState('');\n  const {\n    0: nickname,\n    1: setNickname\n  } = useState('');\n  const {\n    0: birth,\n    1: setBirth\n  } = useState('');\n  const {\n    0: phone,\n    1: setPhone\n  } = useState('');\n  useEffect(() => {\n    if (isSignUpError) {\n      alert(isSignUpError);\n    }\n  }, [isSignUpError]);\n  useEffect(() => {\n    if (isSignUpDone) {\n      Router.push('/');\n      dispatch({\n        type: SIGN_UP_RESET\n      });\n    }\n  }, [isSignUpDone]);\n  const onChangeEmail = useCallback(e => {\n    setEmail(e.target.value);\n  }, [email]);\n  const onChangePassword = useCallback(e => {\n    setPassword(e.target.value);\n  }, [password]);\n  const onChangeNickname = useCallback(e => {\n    setNickname(e.target.value);\n  }, [nickname]);\n  const onChangeBirth = useCallback(e => {\n    setBirth(e.target.value);\n  }, [birth]);\n  const onChangePhone = useCallback(e => {\n    setPhone(e.target.value);\n  }, [phone]); //로그인 액션 보내고 성공하면 로그인 주소로 Router.replace ㄱㄱ\n\n  const onSubmitForm = useCallback(() => {\n    console.log(email, password, nickname, birth, phone);\n    dispatch({\n      type: SIGN_UP_REQUEST,\n      data: {\n        email,\n        password,\n        nickname\n      }\n    });\n  }, [email, password, nickname, birth, phone]);\n  return /*#__PURE__*/_jsxs(FormWrapper, {\n    onFinish: onSubmitForm,\n    children: [/*#__PURE__*/_jsx(\"label\", {\n      htmlFor: \"user-email\",\n      children: \"\\uC774\\uBA54\\uC77C\"\n    }), /*#__PURE__*/_jsx(\"br\", {}), /*#__PURE__*/_jsx(Input, {\n      name: \"user-email\",\n      placeholder: \"exmple@mju.ac.kr\",\n      value: email,\n      type: \"email\",\n      onChange: onChangeEmail,\n      required: true\n    }), /*#__PURE__*/_jsx(\"label\", {\n      htmlFor: \"user-password\",\n      children: \"\\uBE44\\uBC00\\uBC88\\uD638\"\n    }), /*#__PURE__*/_jsx(\"br\", {}), /*#__PURE__*/_jsx(Input, {\n      name: \"user-password\",\n      type: \"password\",\n      value: password,\n      onChange: onChangePassword,\n      required: true\n    }), /*#__PURE__*/_jsx(\"label\", {\n      htmlFor: \"user-nickname\",\n      children: \"\\uB2C9\\uB124\\uC784\"\n    }), /*#__PURE__*/_jsx(\"br\", {}), /*#__PURE__*/_jsx(Input, {\n      name: \"user-nickname\",\n      value: nickname,\n      onChange: onChangeNickname\n    }), /*#__PURE__*/_jsx(\"label\", {\n      htmlFor: \"user-password\",\n      children: \"\\uC0DD\\uB144\\uC6D4\\uC77C\"\n    }), /*#__PURE__*/_jsx(\"br\", {}), /*#__PURE__*/_jsx(Input, {\n      name: \"user-birth\",\n      placeholder: \"980427\",\n      maxLength: 6,\n      value: birth,\n      onChange: onChangeBirth\n    }), /*#__PURE__*/_jsx(\"label\", {\n      htmlFor: \"user-phone\",\n      children: \"\\uC5F0\\uB77D\\uCC98\"\n    }), /*#__PURE__*/_jsx(\"br\", {}), /*#__PURE__*/_jsx(Input, {\n      name: \"user-phone\",\n      type: \"tel\",\n      placeholder: \"010-1234-5678\",\n      maxLength: 13,\n      value: phone,\n      onChange: onChangePhone\n    }), /*#__PURE__*/_jsx(ButtonWrapper, {\n      children: /*#__PURE__*/_jsx(Button, {\n        htmlType: \"submit\",\n        loading: isSignUpLoading,\n        children: \"\\uD68C\\uC6D0\\uAC00\\uC785\"\n      })\n    })]\n  });\n};\n\nexport default LoginForm;","map":null,"metadata":{},"sourceType":"module"}