{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState, useCallback, useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Form, List, Button, Input } from 'antd';\nimport styled from 'styled-components';\nimport { ADD_BUCKET_REQUEST, REMOVE_BUCKET_REQUEST } from '../reducers/bucket';\nconst CenterDiv = styled.div.withConfig({\n  displayName: \"EditBucket__CenterDiv\",\n  componentId: \"i2oi1u-0\"\n})([\"width:70%;margin:0 auto;margin-top:20px;\"]);\nconst CustomBtn = styled(Button).withConfig({\n  displayName: \"EditBucket__CustomBtn\",\n  componentId: \"i2oi1u-1\"\n})([\"margin-bottom:5px;\"]);\nconst CustomItem = styled(List.Item).withConfig({\n  displayName: \"EditBucket__CustomItem\",\n  componentId: \"i2oi1u-2\"\n})([\"margin:0;justify-content:center;padding:0;\"]);\nconst CustomInput = styled(Input).withConfig({\n  displayName: \"EditBucket__CustomInput\",\n  componentId: \"i2oi1u-3\"\n})([\"margin-bottom:5px;\"]);\n\nconst EditBucket = () => {\n  const dispatch = useDispatch();\n  const {\n    0: content,\n    1: setContent\n  } = useState('');\n  const {\n    bucket\n  } = useSelector(state => state.bucket); //Input\n\n  const onContentChange = useCallback(e => {\n    setContent(e.target.value);\n  }, [content]); //Add Bucket\n\n  const onAddClick = useCallback(() => {\n    const data = {\n      content: content\n    };\n    dispatch({\n      type: ADD_BUCKET_REQUEST,\n      data\n    });\n    setContent('');\n  }, [content]); //Delete Bucket\n\n  const onDeleteClick = useCallback(() => {\n    //find\n    const deleteData = bucket.filter(e => e.content === content)[0];\n\n    if (deleteData) {\n      dispatch({\n        type: REMOVE_BUCKET_REQUEST,\n        data: deleteData.id\n      });\n    } else {\n      alert(\"입력 또는 버킷리스트를 확인하세요\");\n    }\n\n    setContent('');\n  }, [content]);\n  return /*#__PURE__*/_jsx(Form, {\n    children: /*#__PURE__*/_jsxs(CenterDiv, {\n      children: [/*#__PURE__*/_jsx(CustomBtn, {\n        onClick: onAddClick,\n        children: \"\\uCD94\\uAC00\"\n      }), /*#__PURE__*/_jsx(CustomBtn, {\n        onClick: onDeleteClick,\n        style: {\n          float: 'right'\n        },\n        children: \"\\uC0AD\\uC81C\"\n      }), /*#__PURE__*/_jsx(CustomInput, {\n        onChange: onContentChange,\n        value: content,\n        placeholder: '추가 또는 삭제하고싶은 버킷리스트를 입력하세요'\n      }), /*#__PURE__*/_jsx(List, {\n        style: {\n          height: '350px',\n          overflow: 'scroll'\n        },\n        size: \"small\",\n        bordered: true,\n        dataSource: bucket,\n        renderItem: item => /*#__PURE__*/_jsx(CustomItem, {\n          children: item.content\n        })\n      })]\n    })\n  });\n};\n\nexport default EditBucket;","map":null,"metadata":{},"sourceType":"module"}